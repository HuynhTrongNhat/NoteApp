<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.notebook">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@drawable/avatar1"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Note_Update_Detail"></activity>
        <activity
            android:name=".Note_View_Detail"
            android:label="@string/title_activity_note__view__detail" />
        <activity
            android:name=".Note_Edit_Detail"
            android:label="@string/title_activity_note__edit__detail" />
        <activity
            android:name=".MainActivity"
            android:label="@string/title_main">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>